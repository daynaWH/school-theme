{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#444444",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#163b6d",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#67113b",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#23579f",
					"name": "Hover",
					"slug": "hover"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "62.5rem"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Noto Sans\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/noto-sans/noto-sans-400-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Noto Sans\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/noto-sans/noto-sans-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Noto Sans\", sans-serif",
					"name": "Noto Sans",
					"slug": "noto-sans"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Noto Serif\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/noto-serif/noto-serif-700-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Noto Serif\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/noto-serif/noto-serif-400-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Noto Serif\", serif",
					"name": "Noto Serif",
					"slug": "noto-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.15rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--accent)",
								"style": "solid",
								"width": "2px"
							},
							"left": {
								"color": "var(--wp--preset--color--accent)",
								"style": "solid",
								"width": "2px"
							},
							"right": {
								"color": "var(--wp--preset--color--accent)",
								"style": "solid",
								"width": "2px"
							},
							"top": {
								"color": "var(--wp--preset--color--accent)",
								"style": "solid",
								"width": "2px"
							}
						},
						"color": {
							"background": "var(--wp--preset--color--base)",
							"text": "var(--wp--preset--color--accent)"
						}
					}
				}
			},
			"core/pullquote": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--70)",
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--70)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/site-title": {
				"spacing": {
					"margin": {
						"left": "var(--wp--preset--spacing--40)"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3rem"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.25rem"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.5rem"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--hover)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-sans)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--40)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-sans)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "400"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}